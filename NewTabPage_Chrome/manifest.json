{
	"name": "New Tab Page",
	"version": "1.1",
	"manifest_version": 2,
	"description": "Redesigned new tab page featuring your bookmarks, most visited",
	"icons": {
		"128": "icon_128.png",
		 "48": "icon_48.png",
		 "16": "icon_16.png"
	},
	"permissions": [
		"bookmarks",
		"topSites",
		"management",
		"tabs",
		"sessions"
	],
	"chrome_url_overrides":
	{
		"newtab": "newtab.html"
	},
	"background" : {
		"scripts" : ["background.js"],
		"persistent" : true
	},
	"options_ui": {
		"page": "newtab.html#options",
		"chrome_style": false,
		"open_in_tab": true
	},
	"web_accessible_resources": [
		"newtab.html"
	]
}
